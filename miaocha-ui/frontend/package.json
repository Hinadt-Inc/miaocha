    "test": "vitest",
    "clean": "rm -rf node_modules yarn.lock pnpm-lock.yaml"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": ["eslint --fix", "prettier --write"],
    "*.{css,less,json}": ["prettier --write"]
