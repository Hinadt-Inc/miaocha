{
  "name": "miaocha-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:no-tsc": "vite build",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,css,less,json}\"",
    "prepare": "husky install .husky || echo 'husky was failed, skiped'",
